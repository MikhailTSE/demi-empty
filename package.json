{
  "name": "demi-empty",
  "version": "0.0.5",
  "description": "Empty JS objects",
  "keywords": [
    "empty",
    "empty objects",
    "noop"
  ],
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "clean": "rm -rf dist",
    "build": "npm test && npm run clean && rollup -c",
    "link": "npm run build && cd dist && npm link"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikhailTSE/demi-empty.git"
  },
  "author": "Mikhail",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MikhailTSE/demi-empty/issues"
  },
  "homepage": "https://github.com/MikhailTSE/demi-empty#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-jest": "^25.3.0",
    "eslint": "^6.8.0",
    "jest": "^25.3.0",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/__tests__/utils.js"
    ]
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ]
}
