{
  "name": "demi-empty",
  "version": "0.0.2",
  "description": "Empty JS objects",
  "keywords": [
    "empty",
    "empty objects",
    "noop"
  ],
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "clean": "rm -rf dist",
    "build": "npm test && npm run clean && parcel build ./src/index.js",
    "link": "npm run build && cd dist && npm link"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikhailTSE/demi-empty.git"
  },
  "author": "Mikhail",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MikhailTSE/demi-empty/issues"
  },
  "homepage": "https://github.com/MikhailTSE/demi-empty#readme",
  "devDependencies": {
    "eslint": "^6.7.2",
    "jest": "^24.9.0",
    "parcel-bundler": "^1.12.4"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/__tests__/utils.js"
    ]
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "registry":"https://npm.pkg.github.com/"
  }
}
